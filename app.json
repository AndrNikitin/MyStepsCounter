{
  "expo": {
    "name": "MyStepsCounter",
    "slug": "MyStepsCounter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "Steps and Location"
    },
    "permissions": ["location", "location-background", "pedometer"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.qweasd555.myapp"
    },
    "extra": {
      "eas": {
        "projectId": "225f604e-68a7-4f07-beeb-877df8faf83e"
      }
    }
  }
}
